{
	"name": "@ctx-core/auth0-ui",
	"version": "9.0.15",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"browser": "./src/browser/index.js",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepare": "npm run build",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^24.0.13",
		"@ctx-core/auth0-management": "workspace:^5.0.11",
		"@ctx-core/dialog": "workspace:^14.0.9",
		"@ctx-core/dom": "workspace:^10.0.5",
		"@ctx-core/error": "workspace:^10.0.4",
		"@ctx-core/fetch": "workspace:^10.0.9",
		"@ctx-core/jwt": "workspace:^9.0.5",
		"@ctx-core/object": "workspace:^20.0.5",
		"@ctx-core/store": "workspace:^26.0.6",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.6",
		"@ctx-core/dev-tools": "workspace:^6.0.5",
		"@ctx-core/function": "workspace:^19.1.4",
		"@types/jsonwebtoken": "^8.5.2",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}