{
	"name": "@ctx-core/auth0-ui",
	"version": "12.2.58",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"svelte": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "^34.2.3",
		"@ctx-core/auth0-management": "^9.0.43",
		"@ctx-core/dialog": "^20.0.13",
		"@ctx-core/dialog-ui-svelte": "^1.0.15",
		"@ctx-core/dom": "^11.1.9",
		"@ctx-core/error": "^11.4.1",
		"@ctx-core/fetch-undici": "^1.2.14",
		"@ctx-core/function": "^20.6.0",
		"@ctx-core/http": "^9.0.24",
		"@ctx-core/jwt": "^10.1.3",
		"@ctx-core/logger": "^9.0.44",
		"@ctx-core/object": "^22.1.3",
		"@ctx-core/svelte": "^10.1.11",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.165",
		"auth0-js": "^9.19.0",
		"jsonwebtoken": "^8.5.1",
		"polka": "next"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/auth0-js": "^9.14.6",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/trouter": "^3.1.1",
		"rimraf": "^3.0.2",
		"svelte": "^3.47.0",
		"svelte-check": "^2.6.0",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.5.8",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}