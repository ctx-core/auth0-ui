{
	"name": "@ctx-core/auth0-ui",
	"version": "12.2.144",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"svelte": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "^35.0.34",
		"@ctx-core/auth0-management": "^9.0.87",
		"@ctx-core/dialog": "^20.1.16",
		"@ctx-core/dialog-ui-svelte": "^1.0.38",
		"@ctx-core/dom": "^11.2.2",
		"@ctx-core/error": "^11.5.2",
		"@ctx-core/fetch-undici": "^2.3.17",
		"@ctx-core/function": "^21.6.0",
		"@ctx-core/http": "^9.0.32",
		"@ctx-core/jwt": "^10.1.23",
		"@ctx-core/logger": "^9.0.45",
		"@ctx-core/object": "^22.5.0",
		"@ctx-core/svelte": "^13.4.7",
		"@swc/cli": "^0.1.59",
		"@swc/core": "^1.3.25",
		"auth0-js": "^9.20.0",
		"jsonwebtoken": "^9.0.0",
		"polka": "next"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/auth0-js": "^9.14.7",
		"@types/jsonwebtoken": "^9.0.0",
		"@types/trouter": "^3.1.1",
		"rimraf": "^3.0.2",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.0",
		"svelte2tsx": "^0.6.0",
		"tslib": "^2.4.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}