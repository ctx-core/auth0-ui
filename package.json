{
	"name": "@ctx-core/auth0-ui",
	"version": "9.0.29",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"browser": "./src/browser/index.js",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^24.0.28",
		"@ctx-core/auth0-management": "workspace:^5.0.24",
		"@ctx-core/dialog": "workspace:^14.0.23",
		"@ctx-core/dom": "workspace:^10.0.15",
		"@ctx-core/error": "workspace:^10.1.5",
		"@ctx-core/fetch": "workspace:^10.0.20",
		"@ctx-core/jwt": "workspace:^9.0.13",
		"@ctx-core/object": "workspace:^20.2.2",
		"@ctx-core/store": "workspace:^26.0.18",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.13",
		"@ctx-core/dev-tools": "workspace:^6.0.15",
		"@ctx-core/function": "workspace:^19.1.10",
		"@types/jsonwebtoken": "^8.5.2",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
