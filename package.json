{
	"name": "@ctx-core/auth0-ui",
	"version": "12.2.0",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"svelte": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^28.1.0",
		"@ctx-core/auth0-management": "workspace:^7.1.0",
		"@ctx-core/dialog": "workspace:^18.0.16",
		"@ctx-core/dialog-ui-svelte": "workspace:^1.0.14",
		"@ctx-core/dom": "workspace:^11.1.5",
		"@ctx-core/error": "workspace:^11.1.23",
		"@ctx-core/fetch-undici": "workspace:^1.0.0",
		"@ctx-core/function": "workspace:^20.4.0",
		"@ctx-core/http": "workspace:^9.0.22",
		"@ctx-core/jwt": "workspace:^10.0.50",
		"@ctx-core/logger": "workspace:^9.0.44",
		"@ctx-core/object": "workspace:^22.0.13",
		"@ctx-core/svelte": "workspace:^10.1.7",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.133",
		"auth0-js": "^9.18.1",
		"jsonwebtoken": "^8.5.1",
		"polka": "next"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/trouter": "^3.1.1",
		"rimraf": "^3.0.2",
		"svelte": "^3.46.2",
		"svelte-check": "^2.3.0",
		"svelte-preprocess": "^4.10.2",
		"svelte2tsx": "^0.4.14",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}
