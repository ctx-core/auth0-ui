{
	"name": "@ctx-core/auth0-ui",
	"version": "10.0.0",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"browser": "./src/browser/index.js",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^25.0.0",
		"@ctx-core/auth0-management": "workspace:^6.0.0",
		"@ctx-core/dialog": "workspace:^15.0.0",
		"@ctx-core/dom": "workspace:^11.0.0",
		"@ctx-core/error": "workspace:^11.0.0",
		"@ctx-core/fetch": "workspace:^11.0.0",
		"@ctx-core/jwt": "workspace:^10.0.0",
		"@ctx-core/object": "workspace:^21.0.0",
		"@ctx-core/store": "workspace:^27.0.0",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.25",
		"@ctx-core/dev-tools": "workspace:^7.0.0",
		"@ctx-core/function": "workspace:^20.0.0",
		"@types/jsonwebtoken": "^8.5.4",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}