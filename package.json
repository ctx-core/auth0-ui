{
	"name": "@ctx-core/auth0-ui",
	"version": "12.1.10",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^27.0.7",
		"@ctx-core/auth0-management": "workspace:^7.0.7",
		"@ctx-core/dialog": "workspace:^17.0.6",
		"@ctx-core/dom": "workspace:^11.0.46",
		"@ctx-core/error": "workspace:^11.1.15",
		"@ctx-core/fetch": "workspace:^11.0.99",
		"@ctx-core/function": "workspace:^20.2.18",
		"@ctx-core/http": "workspace:^9.0.16",
		"@ctx-core/jwt": "workspace:^10.0.44",
		"@ctx-core/logger": "workspace:^9.0.38",
		"@ctx-core/object": "workspace:^22.0.6",
		"@ctx-core/store": "workspace:^27.0.53",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"auth0-js": "^9.18.0",
		"jsonwebtoken": "^8.5.1",
		"polka": "next"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.93",
		"@ctx-core/dialog-ui-svelte": "workspace:^1.0.7",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/trouter": "^3.1.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.10",
		"svelte-preprocess": "^4.10.0",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
