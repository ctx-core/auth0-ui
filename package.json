{
	"name": "@ctx-core/auth0-ui",
	"version": "12.2.59",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"svelte": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^34.2.4",
		"@ctx-core/auth0-management": "workspace:^9.0.44",
		"@ctx-core/dialog": "workspace:^20.0.14",
		"@ctx-core/dialog-ui-svelte": "workspace:^1.0.16",
		"@ctx-core/dom": "workspace:^11.1.10",
		"@ctx-core/error": "workspace:^11.4.2",
		"@ctx-core/fetch-undici": "workspace:^1.2.15",
		"@ctx-core/function": "workspace:^20.6.1",
		"@ctx-core/http": "workspace:^9.0.25",
		"@ctx-core/jwt": "workspace:^10.1.4",
		"@ctx-core/logger": "workspace:^9.0.45",
		"@ctx-core/object": "workspace:^22.1.4",
		"@ctx-core/svelte": "workspace:^10.1.12",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.165",
		"auth0-js": "^9.19.0",
		"jsonwebtoken": "^8.5.1",
		"polka": "next"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/auth0-js": "^9.14.6",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/trouter": "^3.1.1",
		"rimraf": "^3.0.2",
		"svelte": "^3.47.0",
		"svelte-check": "^2.6.0",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.5.8",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}
