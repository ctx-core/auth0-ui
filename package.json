{
	"name": "@ctx-core/auth0-ui",
	"version": "8.0.5",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-ui.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"browser": "./dist/browser/index.js",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile && npm run symlink",
		"clean": "rm -f tsconfig.tsbuildinfo",
		"compile": "./node_modules/.bin/tsc -b .",
		"symlink": "./node_modules/.bin/symlink_src_assets_to_dist .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^23.0.5",
		"@ctx-core/auth0-management": "workspace:^4.0.4",
		"@ctx-core/dialog": "workspace:^12.0.6",
		"@ctx-core/dom": "workspace:^9.0.2",
		"@ctx-core/error": "workspace:^9.0.0",
		"@ctx-core/fetch": "workspace:^9.0.1",
		"@ctx-core/jwt": "workspace:^8.0.1",
		"@ctx-core/object": "workspace:^19.0.2",
		"@ctx-core/store": "workspace:^25.1.0",
		"jsonwebtoken": "^8.5.1",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^7.0.2",
		"@ctx-core/dev-tools": "workspace:^5.0.2",
		"@ctx-core/function": "workspace:^18.0.1",
		"@types/jsonwebtoken": "^8.5.1",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
